
#@btForm()
<div class="well well-sm form-inline search-form clearfix">
    <div class="search-boxitems col-md-4">
        <label class="col-md-3 pl-0 pr-0">分类编号:</label>
        <div class="col-md-9 pl-0">#treeSelectFix("searchKindCode" "" {"class":"input-text","readonly":"readonly","data-URL":"/syskind/getTreeData","style":"width:100%"})</div>
    </div>
    <div class="search-boxitems col-md-4">
        <label class="col-md-3 pl-0 pr-0">条目编号:</label>
        <div class="col-md-9 pl-0">#tInput("dictEntryCode" "" {"size":"10","class":"input-sm w-percent"})</div>
    </div>
    <div class="search-boxitems col-md-4">
        <label class="col-md-3 pl-0 pr-0">条目名称:</label>
        <div class="col-md-9 pl-0">#tInput("dictEntryName" "" {"size":"10","class":"input-sm w-percent"})</div>
    </div>
    <div class="search-boxitems col-md-4">
        #@tButton("btn-primary btn-sm")#faIcon("search")&nbsp;搜索#end
        <input type="reset" class="btn btn-default btn-sm" value="重置" />
    </div>
</div>
#end
#sReset()