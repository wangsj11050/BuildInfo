<script type="text/javascript">
	$(function(){
		$(".function1 div span").click(function(){
			var _this = $(this);
			var adisverid = _this.siblings("input[name=adisverid]").val();
			var adisvername = _this.siblings("input[name=adisvername]").val();
			var demand = _this.siblings("input[name=demand]").val();
			var phone = _this.siblings(".form-control");
			
			if(phone.val() == ''){
				layer.msg("请输入预留电话!", {time:3000,icon: 2});
	            phone.focus();  
	            return false;
			}
			$.ajax({
				url:"#link('/pubdemand/pubdemand')",
				type:'POST',
				data:{
					name:'无_客户未预留姓名',
					demand:demand,
					adisverId:adisverid,
					adisverName:adisvername,
					phone:phone.val()
				},
				dataType:'json',
				success:function(data){
					if (data.status == "y") {
                        layer.msg(data.info,{time:4000});
                    } else {
                        layer.msg(data.info, {time:4000,icon: 2});
                    }
				},
				error: function () {
                    layer.msg(data.info, {time:4000,icon: 2});
                }
			})
			
			phone.val("");
		});
	});
</script>